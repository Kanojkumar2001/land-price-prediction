{
  "name": "land-price-prediction-app",
  "version": 2,
  "builds": [
    {
      "src": "api/predict.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/test.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.py"
    },
    {
      "src": "/api/locations",
      "dest": "/api/predict.py"
    },
    {
      "src": "/api/predict",
      "dest": "/api/predict.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}
